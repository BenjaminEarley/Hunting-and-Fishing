{
	"type":"object",
	"title":"hunting",
	"$schema": "http://json-schema.org/draft-03/schema",
	"properties":{
		"agency": {
			"type":"string",
			"required":true
		},
		"date_effective": {
			"type":"string",
			"format":"date-time",
			"required":true
		},
		"date_expires": {
			"type":"string",
			"format":"date-time",
			"required":true
		},
		"documentation": {
			"type":"string",
			"format":"uri",
			"required":true
		},
		"schema_version": {
			"type":"string",
			"required":true
		},
		"conformsTo": {
			"type":"string",
			"format":"uri",
			"required":true
		},
		"state": {
			"type":"string",
			"minLength":2,
			"maxLength":2,
			"pattern":"([A-Z]{2})",
			"required": true
		},
		"seasons": {
			"type":"object",
			"required":true,
			"properties":{
				"season": {
					"type":"object",
					"required":true,
					"properties":{
						"name": {
							"type":"string",
							"required":false
						},
						"comment": {
							"type":"string",
							"required":false
						},
						"method_rules": {
							"type":"array",
							"required":false,
							"items": {
								"type":"string",
								"required":true
							}
						},
						"method": {
							"type": ["string", "array"],
							"required":true
						},
						"range": {
							"type":"array",
							"required":false,
							"properties":{
								"1": {
									"type":"object",
									"required":false,
									"properties":{
										"places": {
											"type":"object",
											"required":false
										},
										"season": {
											"type":"object",
											"required":false,
											"properties":{
												"date": {
													"type":"object",
													"required":false,
													"properties":{
														"ends": {
															"type":"string",
															"required":false
														},
														"starts": {
															"type":"string",
															"required":false
														}
													}
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		},
		"species": {
			"type":"object",
			"required":false,
			"properties":{
				"name": {
					"type":"string",
					"required":true
				},
				"bova_id": {
					"type":"string",
					"required":false
				},
				"taxonomy": {
					"type":"string",
					"required":false
				},
				"url": {
					"type":"string",
					"format":"uri",
					"required":false
				},
				"aliases": {
					"type":"array",
					"required":false
				},
				"members": {
					"type":"array",
					"required":false,
					"properties":{
						"name": {
							"type":"string",
							"required":true
						},
						"bova_id": {
							"type":"string",
							"required":false
						},
						"taxonomy": {
							"type":"string",
							"required":false
						},
						"url": {
							"type":"string",
							"required":false,
							"format":"uri"
						},
						"aliases": {
							"type":"array",
							"required":false
						}
					}
				}
			}
		}
	}
}
